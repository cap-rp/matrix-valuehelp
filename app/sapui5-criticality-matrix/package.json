{
 "name": "sapui5.criticality.matrix",
 "version": "1.0.0",
 "description": "A SAPUI5 application for displaying a criticality matrix with color-coded cells.",
 "main": "webapp/index.html",
 "scripts": {
  "build": "ui5 build --clean-dest",
  "build:opt": "ui5 build self-contained --clean-dest --all",
  "start": "ui5 serve --port 8080 -o index.html",
  "start:dist": "ui5 serve  --port 8080 -o index.html --config ui5-dist.yaml",
  "watch:controls": "npx @ui5/ts-interface-generator --watch",
  "ts-typecheck": "tsc --noEmit",
  "lint": "eslint webapp"
 },
 "devDependencies": {
  "@typescript-eslint/eslint-plugin": "^8.40.0",
  "@typescript-eslint/parser": "^8.40.0",
  "@ui5/cli": "^4.0.23",
  "@ui5/ts-interface-generator": "^0.10.3",
  "eslint": "^9",
  "typescript": "^5.0.3",
  "ui5-middleware-livereload": "^3",
  "ui5-middleware-simpleproxy": "^3",
  "ui5-tooling-transpile": "^3"
 },
 "sapux": [
  "app/admin-books",
  "app/browse"
 ]
}
