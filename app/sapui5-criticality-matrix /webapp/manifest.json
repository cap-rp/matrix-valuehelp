{
  "sap.app": {
    "id": "sapui5.criticality.matrix",
    "type": "application",
    "title": "Criticality Matrix",
    "dataSources": {
      "mainService": {
        "uri": "/odata/v4/criticality/",
        "type": "OData",
        "settings": { "odataVersion": "4.0" }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "rootView": {
      "viewName": "sapui5.criticality.matrix.view.App",
      "type": "XML",
      "id": "app"
    },
    "dependencies": {
      "minUI5Version": "1.60.0",
      "libs": {
        "sap.m": {}
      }
    },
    "models": {
      "": {
        "dataSource": "mainService",
        "settings": { "synchronizationMode": "None" }
      },
      "odata": {
        "dataSource": "mainService",
        "settings": { "odataVersion": "4.0" }
      }
    }
  }
}